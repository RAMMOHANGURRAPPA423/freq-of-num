 #include <stdio.h>
 
 int main()
{
    int a[10],b[10],i,j,n,count=0 ;
   
    printf("Enter size of the array : ");
    scanf("%d", &n);
 
    printf("Enter elements in array : ");
    for(i=0; i<n; i++)
    {
        scanf("%d",&a[i]);
    }
    
  for(i=0; i<n; i++)
    {
        count=1;
        if(a[i]!=-1)
		{
		    for(j=i+1; j<n; j++)
     
            {
        	   if(a[i]==a[j])
        	    {
			       count++;
			       a[j]=-1;
		       }
	       }
	       b[i]=count;
		}
         
    }
     
for(i=0; i<n; i++)
    {
         if(a[i]!=-1)
        {
        	printf("no of %d is %d \n",a[i],b[i]);
 
		}
		
    }
         
    return 0;
}
